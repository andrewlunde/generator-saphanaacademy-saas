{
    "name": "<%= projectName %>-srv",
    "engines": {
        "node": "14.x"
    },
    "dependencies": {
        "express": "latest",
        "cfenv": "latest",
        "body-parser": "latest",
<% if(HANA){ %>
        "@sap/hdbext": "latest",
        "@sap/instance-manager": "latest",
        "axios": "latest",
<% } %>
<% if(routes || SaaSAPI || destination){ %>
        "@sap-cloud-sdk/http-client": "latest",
<% } %>
<% if(destination){ %>
        "@sap-cloud-sdk/connectivity": "latest",
<% } %>
        "@sap/xsenv": "latest",
        "@sap/xssec": "latest",
        "passport": "latest"
    },
    "scripts": {
        "start": "node server.js"
    }
}